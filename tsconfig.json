{
	"compileOnSave": false,
	"compilerOptions": {
		"baseUrl": "./",
		"downlevelIteration": true,
		"outDir": "./dist/out-tsc",
		"sourceMap": true,
		"declaration": false,
		"module": "esnext",
		"moduleResolution": "node",
		"experimentalDecorators": true,
		"importHelpers": true,
		"target": "es2015",
		"typeRoots": ["node_modules/@types"],
		"lib": ["es2018", "dom"],
		"paths": {
			"ngx-custom-intl-tel": ["dist/ngx-custom-intl-tel"],
			"ngx-custom-intl-tel/*": ["dist/ngx-custom-intl-tel/*"]
		}
	}
}
