{
	"name": "ngx-custom-intl-tel-app",
	"version": "0.0.1",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"build_stats": "ng build --stats-json",
		"analyze": "webpack-bundle-analyzer dist/ngx-custom-intl-tel-app/stats.json",
		"build_lib": "ng build ngx-custom-intl-tel",
		"copy-license": "cp ./LICENSE ./dist/ngx-custom-intl-tel",
		"copy-readme": "cp ./README.md ./dist/ngx-custom-intl-tel",
		"copy-files": "npm run copy-license && npm run copy-readme",
		"npm_pack": "npm run copy-files && cd dist/ngx-custom-intl-tel && npm pack",
		"package": "npm run build_lib && npm run npm_pack",
		"postinstall": "ngcc"
	},
	"private": false,
	"dependencies": {
		"google-libphonenumber": "^3.2.3",
		"intl-tel-input": "^17.0.3",
		"ngx-bootstrap": "^6.0.0"
	}
}
